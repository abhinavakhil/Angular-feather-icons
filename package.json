{
  "name": "workspace",
  "version": "1.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lib:copy-assets": "cp README.md LICENSE dist/angular-feather-icons",
    "lib:build": "ng build angular-feather-icons --configuration production && npm run lib:copy-assets",
    "lib:pack": "cd dist/angular-feather-icons && npm pack",
    "lib:publishLocally": "npm run lib:build && npm run lib:pack",
    "lib:publish": "npm publish dist/angular-feather-icons",
    "lib:test": "ng test angular-feather-icons --code-coverage",
    "lib:test-headless": "ng test angular-feather-icons --code-coverage --watch=false --browsers=ChromeHeadless",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "17.0.1",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/schematics": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/animations": "^17.0.2",
    "@angular/cli": "^17.0.0",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/compiler-cli": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@types/jasmine": "~5.1.2",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "del": "^7.1.0",
    "eslint": "^8.53.0",
    "fs-extra": "^11.1.1",
    "gb-feather-icons": "^1.0.1",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^17.0.0",
    "rxjs": "^7.8.1",
    "ts-node": "~10.9.1",
    "tslib": "^2.6.2",
    "typescript": "~5.2.2",
    "uppercamelcase": "^3.0.0",
    "zone.js": "~0.14.2"
  }
